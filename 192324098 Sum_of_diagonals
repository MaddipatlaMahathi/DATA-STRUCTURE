#include<stdio.h>
int main()
{
	int i,j,n,m;
	int major=0,minor=0;
	printf("\nEnter the rows: ");
	scanf("%d",&n);
	printf("\nEnter col: ");
	scanf("%d",&m);
int a[n][m];
printf("\nEnter the elements:");
for(i=0;i<n;i++)
{
	for(j=0;j<m;j++)
	{
		scanf("%d",&a[i][j]);
	}
	}
	for(i=0;i<n;i++)
	{
	
	major+=a[i][i];
	minor+=a[i][n-i-1];
}

	printf("\nSum of major: %d",major);
	printf("\nSum of minor:Â %d",minor);

}
